CC = gcc
SRC = main.c holberton.c
OBJ = $(SRC:.c=.o) 
NAME = holberton
TOCLEAN = *~ 
CFLAGS = -Wall -Werror -Wextra -pedantic

.PHONY: all clean fclean oclean re

all : $(OBJ) m.h
	$(CC) $(OBJ) -o $(NAME)

fclean: oclean clean

oclean:
	$(RM) $(OBJ)

clean :
	$(RM)  $(TOCLEAN) $(NAME)
re: fclean all
